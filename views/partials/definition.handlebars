{{#if params}}
  <h4 class="u-h4">Parameters<h4>
  {{#each params}}
    <div class="c-def">
      <div class="c-def__title">{{title}}</div>
      <div class="c-def__desc">{{desc}} 
        {{#if example}}<span class="u-muted"><span class="u-text-s">e.g.</span> {{example}}</span>{{/if}}      
      </div>
    </div>
  {{/each}}
{{/if}}